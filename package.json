{
  "name": "pacelab",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=20.19.0 <21"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "start:go": "CI=false expo start --go",
    "start:go:lan": "bash ./scripts/start-expo-go.sh",
    "stop:expo": "bash ./scripts/stop-expo.sh",
    "start:go:tunnel": "CI=false expo start --go --tunnel",
    "start:go:lan:clear": "CI=false REACT_NATIVE_PACKAGER_HOSTNAME=$(ipconfig getifaddr en0 2>/dev/null || ipconfig getifaddr en1 2>/dev/null) expo start --go --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:node": "node -e \"require('@expo/cli')\" run:ios",
    "ios:debug": "EXPO_DEBUG=1 npx expo run:ios 2>&1",
    "web": "expo start --web",
    "deploy:strava-callback": "npx supabase functions deploy strava-auth-callback --no-verify-jwt",
    "deploy:strava": "npx supabase functions deploy strava-auth-callback --no-verify-jwt && npx supabase functions deploy strava-import-history && npx supabase functions deploy strava-sync-manual --no-verify-jwt && npx supabase functions deploy strava-refresh-token",
    "deploy:strava-sync": "npx supabase functions deploy strava-sync-manual --no-verify-jwt",
    "deploy:session-adaptation": "npx supabase functions deploy analyze-today-session --no-verify-jwt && npx supabase functions deploy calculate-user-baselines --no-verify-jwt",
    "deploy:apple-health-cloud": "npx supabase functions deploy process-apple-health-export && npx supabase functions deploy upload-health-page --no-verify-jwt",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.0.14",
    "@react-navigation/material-top-tabs": "^7.4.13",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.97.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~7.0.1",
    "expo-auth-session": "~6.0.2",
    "expo-device": "~7.0.0",
    "expo-document-picker": "~13.0.2",
    "expo-file-system": "~19.0.21",
    "expo-notifications": "~0.29.0",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~14.0.2",
    "fast-xml-parser": "^4.3.2",
    "fflate": "^0.8.2",
    "jszip": "^3.10.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1314.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-health": "^1.19.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "^13.16.0"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.3.0",
    "@types/react": "~19.1.10",
    "patch-package": "^8.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
